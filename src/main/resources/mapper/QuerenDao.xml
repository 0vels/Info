<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.school.dao.QuerenDao">
    <!--查找-->
    <select id="findOneById" resultType="Queren" parameterType="String">
    SELECT
    *
    FROM
    `queren`
    WHERE
    TID = #{TID}
</select>

    <select id="findAll" resultType="Queren" parameterType="String">
    SELECT
    *
    FROM
    `queren`
    </select>

    <!--增加语句-->
    <insert id="add" parameterType="Queren">
        INSERT INTO
        `queren`
        (`TID`,`xuehao`,`userid`,`queren`)
        VALUES
        (#{TID},#{xuehao},#{userid},#{queren})
    </insert>

    <!-- 删除 -->
    <update id="del" parameterType="Queren">
        DELETE FROM
        `queren`
        WHERE
        `TID`=#{TID};
    </update>


</mapper>